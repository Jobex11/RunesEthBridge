{
  "version": 2,
  "builds": [
    {
      "src": "database/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "database/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/database/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/database/public/index.html"
    }
  ]
}
